@page "/characters/{gameName}/new"
@page "/characters/{gameName}/{characterId}/edit"
@using Larp.Landing.Client.Pages.MwFifth

<PageHeader Container>Character Editor</PageHeader>

<MudContainer MaxWidth="MaxWidth.Medium">
    @if (GameName == "mw5e")
    {
        <MwFifthCharacterEditor CharacterId="@CharacterId"/>
    }
</MudContainer>

@code {

    [Parameter]
    public string? GameName { get; set; }

    [Parameter]
    public string? CharacterId { get; set; }

}